CC	Makefile	/^CC = g++ -g -Wall -ggdb -std=c++0x$/;"	m
CFLAGS	Makefile	/^CFLAGS = -c $(INCLUDES)$/;"	m
LIBS	Makefile	/^LIBS =  -lGLEW -lGL -lglfw -lGLU$/;"	m
LoadShaders	src/ShaderUtils.cpp	/^GLuint LoadShaders(std::string szVertexFilePath, std::string szFragmentFilePath) {$/;"	f
